{
    "name": "redux-connected-devtools",
    "version": "0.0.14",
    "license": "MIT",
    "description": "Styless UI",
    "main": "lib/index.js",
    "files": [
        "lib/",
        "lib/index.css",
        "esm/"
    ],
    "types": "lib/index.d.ts",
    "typings": "lib/index.d.ts",
    "scripts": {
        "test": "jest --config jest.config.ts",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "lint": "eslint {src,tests,stories}/**/*.{ts,tsx}",
        "lint:fix": "yarn lint --fix",
        "lint:types": "tsc --noEmit",
        "build:cjs": "tsc",
        "build:es": "tsc -m esNext --outDir esm",
        "build": "node scripts/generate-index.js && npm run clean && npm run scss && yarn build:cjs && yarn build:es && yalc push",
        "clean": "rm -rf lib2/* && rm -rf esm2/*",
        "watch": "nodemon -x \"npm run build\"",
        "scss": "node scripts/build-scss.js"
    },
    "dependencies": {
        "@testing-library/react": "^11.2.5",
        "classnames": "^2.3.1",
        "date-fns": "^2.22.1",
        "igrid": "file:.yalc/igrid",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-draggable": "^4.4.3",
        "react-json-view": "^1.21.1",
        "react-modal": "^3.14.3",
        "react-redux": "^7.2.4",
        "react-toggle": "^4.1.2",
        "react-use": "^17.2.4",
        "react-window": "^1.8.6",
        "redux-connected": "file:.yalc/redux-connected",
        "reselect": "^4.0.0",
        "sass": "^1.32.8"
    },
    "peerDependencies": {
        "redux-connected": "^0.0.10"
    },
    "devDependencies": {
        "@babel/core": "^7.13.8",
        "@jest/types": "^26.6.2",
        "@types/classnames": "^2.2.11",
        "@types/jest": "^26.0.21",
        "@types/lodash": "^4.14.168",
        "husky": "^5.2.0",
        "jest": "^26.6.3",
        "lint-staged": "^10.5.4",
        "prettier": "^2.2.1",
        "ts-jest": "^26.5.4",
        "tslib": "^2.1.0",
        "tslint": "^6.1.3",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^4.2.3"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged",
            "pre-push": "yarn lint && yarn clean && yarn build && yarn test"
        }
    },
    "lint-staged": {
        "src/**/**/*.{ts,tsx}": [
            "eslint --fix",
            "git add"
        ]
    }
}
