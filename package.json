{
    "name": "redux-connected-root",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "git@github.com:dht/redux-connected.git",
    "license": "MIT",
    "private": true,
    "workspaces": {
        "packages": [
            "adapters/*",
            "connectors/*",
            "examples/*",
            "packages/*",
            ".yalc/*",
            ".yalc/@*/*"
        ],
        "nohoist": []
    },
    "scripts": {
        "client": "yarn workspace client start",
        "server": "yarn workspace server start",
        "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\"",
        "clean": "node scripts/clean",
        "link": "./scripts/link.sh"
    },
    "devDependencies": {
        "fs-extra": "^10.0.0",
        "globby": "^11.0.4"
    },
    "dependencies": {}
}
